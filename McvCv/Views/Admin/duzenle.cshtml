@model McvCv.Models.Entity.TblAdmin
@{
    ViewBag.Title = "duzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h4>Şifre Güncelleme Sayfası</h4>
<br />
<form method="post">
    
    @Html.Label("Kullanıcı Adı")
    @Html.TextBoxFor(x => x.username, new { @class = "form-control" })
    <br />
    @Html.Label("Şifre")
    @Html.TextBoxFor(x => x.password, new { @class = "form-control" })
    <br />
  
    <button class="btn btn-info">Güncelle</button>
</form>



